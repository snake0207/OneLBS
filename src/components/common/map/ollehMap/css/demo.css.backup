html {
    height: 100%;
}

body {
    height: 100%;
    width: 100%;
    margin: 0;
    padding: 0;
}

#map {
    width: 100%;
    height: 92%;
}

#markerDecArea {
    top: 80px;
    width: 150px;
    text-align: left;
    background-color: rgba(219, 219, 219, 0.86);
    /*#dbdbdbdb*/
    font-size: 9px;
    margin: 10px;
    padding: 10px;
    z-index: 99999;
}

#markerDecArea ul {
    list-style: none;
    margin: 0;
    padding: 0;
}

#markerDecArea ul img {
    vertical-align: middle;
    width: 20px;
    margin: 5px;
}

#cbArea {
    top: 320px;
    width: 150px;
    text-align: left;
    background-color: rgba(199, 202, 211, 0.86);
    font-size: 9px;
    margin: 10px;
    padding: 10px;
    z-index: 99999;
}

#cbArea ul {
    list-style: none;
    margin: 0;
    padding: 0;
}

#cbArea ul li input {
    vertical-align: middle;
}